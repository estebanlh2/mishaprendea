function MishaNumber(elnumero) {
  if(elnumero <= 10){
    return ("low");
  } else if (elnumero <= 20) {
    return ("Normal");
  } else if (elnumero > 20) {
    return ("High")
  }
}

let randomNumber = Math.floor (Math.random() * 30);

console.log(randomNumber)
console.log(MishaNumber(randomNumber))
